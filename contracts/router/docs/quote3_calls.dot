digraph {
    compound = true;
    
    sdk [label = "SDK";];
    router [label = "Destination Chain\nRouterV2";shape = rect;];
    getAmountOut [label = <<I>3. getAmountOut<br></br>(bridgeToken, tokenOut,<br></br>amountIn - feeAmount)</I>>;shape = none;];
